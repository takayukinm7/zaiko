<%= form_with(url: search_parts_path, locals: true, method: :get, class: "search-form") do |form| %>
  <%= form.text_field :keyword, placeholder: "部品を検索する", class: "search-input" %>
  <%= form.submit "検索", class:"search-btn" %>
<% end %>

<div class="contents row">
  <% @parts.each do |part| %>
    <%= render partial: "part" , locals: { part: part } %>
  <% end %>
</div>